# IGNORE

int main()
{
    crow::SimpleApp app;

    /
    CROW_ROUTE(app, "/")([]() {
        return ".,";
    });

    CROW_ROUTE(app, "/add/<int:x>/<int:y>")([](int x, int y) {
        return crow::response(std::to_string(x + y));
    });

    CROW_ROUTE(app, "/subtract/<int:x>/<int:y>")([](int x, int y) {
        return crow::response(std::to_string(x - y));
    });

    app.port(18080).multithreaded().run();

    return 0;
}
